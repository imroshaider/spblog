<p id="notice"><%= notice %></p>

<h1>Articlees</h1>

<table>
  <thead>
    <tr>
      <th>Titile</th>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articlees.each do |articlee| %>
      <tr>
        <td><%= articlee.titile %></td>
        <td><%= articlee.post %></td>
        <td><%= articlee.user_id %></td>
        <td><%= link_to 'Show', articlee %></td>
        <td><%= link_to 'Edit', edit_articlee_path(articlee) %></td>
        <td><%= link_to 'Destroy', articlee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Articlee', new_articlee_path %>
